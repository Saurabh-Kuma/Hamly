<h2 class="green">History</h2>
{{#each history}}
<button type="button" onclick='putTextAndResult(`{{this.text}}`, `{{this.result}}`) ' id="historyButton" class="btn btn-outline-dark" style="width: 100%; height: 60px; border-color: #40AE49; overflow:hidden;">
   <span class="d-inline-block text-truncate green" style="max-width: 300px; max-height:50px; white-space: normal;">
      {{this.text}}
   </span>
</button>
{{/each}}

<input type="hidden" id="hidden" value='{{name}}@{{total}}@{{spam}}'>